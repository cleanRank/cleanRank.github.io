/* pages/vip/ImSearchPage/ImSearchPage.wxss */
.searchWrap {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
}
.searchHeader {
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 94%;
    margin: 0 auto;
} 
.searchArea {
    width: 100%;
    max-width: 616rpx;
}
.searchBtn {
    color: #777777;
    font-size: 30rpx;
}
/* 搜索结果 */
.friendItem{
    display: flex;
    justify-content: flex-start;
    align-items: center;
    padding: 0 24rpx 0 26rpx;
}
.friendItem image{
    width: 104rpx;
    height: 104rpx;
    margin-right: 22rpx;
    border-radius: 50%;
}
.friendItem .friendName{
    font-size: 30rpx;
    color: #333;
    height: 42rpx;
    padding: 45rpx 0 45rpx;
    flex: 1;
    border-bottom: 1px solid #F4F4F4;
}
/* 无数据缺省 */
.noData{
    display: flex;
    justify-content: flex-start;
    flex-direction: column;
    align-items: center;
}
.noData image{
    width: 320rpx;
    height: 215rpx;
    margin:428rpx 0 96rpx;
}
.noData .noDataInfo{
    font-size: 28rpx;
    height: 40rpx;
    color: #999;
}