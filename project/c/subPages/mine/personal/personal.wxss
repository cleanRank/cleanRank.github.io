.page{
  width: 100%;
  height: 100%;
}
.personal_item{
  height: 110rpx;
  line-height: 110rpx;
  border-bottom: 1px solid #f8f8f8;
  padding: 0 40rpx 0 38rpx;
}
.personal_left, .personal_rig{
  font-size: 28rpx;
}
.personal_rig{
  text-align: right;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.personal_nickName{
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.personal_rig image{
  width: 70rpx;
  height: 70rpx;
  border-radius: 35rpx;
  margin-top: 18rpx;
  border: 2rpx solid #f8f8f8;
}
.editNickNameWrap{
  position: fixed;
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.03);
}
.editNameContent{
  width: 510rpx;
  height: 302rpx;
  border-radius: 12rpx;
  background: #fff;
  display: flex;
  flex-direction: column;
}
.editTitle{
  color: #999;
  font-size: 34rpx;
  line-height: 38rpx;
  padding: 32rpx 0;
  text-align: center;
}
.editUserName{
  display: block;
  width: 285rpx;
  height: 66rpx;
  border-radius: 6rpx;
  background: #F5F5F5;
  margin: 0 auto;
  font-size: 26rpx;
  padding: 0 10rpx;
}
.editUserNameIpt{
  display: block;
  width: 285rpx;
  height: 66rpx;
  border: none;
  text-align: center;
  margin: 0 auto;
  font-size: 26rpx;
}
.editControl{
  border-top-width: 2rpx;
  border-top-style: solid;
  border-top-color: #F8F8F8;
  margin-top: auto;
  position: relative;

}
.editControl:after{
  content: '';
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  height: 100%;
  width: 2rpx;
  background: #F8F8F8;
}
.editBtn{
  color: #777;
  display: block;
  flex: 1;
  text-align: center;
  font-size: 28rpx;
  height: 80rpx;
  line-height: 80rpx;
}
.confirm{
  color: #63ACFD;
}