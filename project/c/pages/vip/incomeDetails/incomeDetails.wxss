.time_list_tab{
  text-align: center;
  padding: 18rpx 0 24rpx;
}
.time_list_tab text{
  font-size: 28rpx;
  color: #777777;
  border-bottom: 4rpx solid #fff;
  line-height: 1.2;
  padding-bottom: 24rpx;
}
.time_list_tab text.tab_a{
  color: #333;
  border-bottom: 4rpx solid #FF3030;
}
.fans_number_box{
   width: 702rpx;
  background: #FFFFFF;
  box-shadow: 0 0 12rpx 0 rgba(0,0,0,0.08);
  border-radius: 12rpx;
  margin: 30rpx auto 60rpx;
  text-align: center;
  padding: 24rpx 0 30rpx;
  position: relative;
}
.fans_number_box .explain_txt{
  position: absolute;
  right: 0;
  top: 46rpx;
  width: 126rpx;
  height: 50rpx;
  line-height: 50rpx;
  background-image: linear-gradient(51deg, #FC6C6A 0%, #FF1F49 94%);
  font-size: 24rpx;
  color: #FFFFFF;
  text-align: center;
  border-radius: 25rpx 0 0 25rpx;
}
.fans_number_box view{
  line-height: 1.8;
}
.fans_num{
  font-size: 80rpx;
  color: #333;
}
.my_fans{
  font-size: 28rpx;
}
.order_rebate_title{
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 1rpx solid #F8F8F8;
  padding-left: 24rpx;
}
.dd_fl{
  font-size: 30rpx;
}
.sxtj_btn{
  text-align: right;
}
.up_v{
padding: 0 24rpx 0 12rpx;
}
.bhth_fl{
  font-size: 24rpx;
}
.order_rebate_list{
  padding-bottom: 24rpx;
  margin-left: 24rpx;
  border-bottom: 1rpx solid #F8F8F8;
}
.order_rebate_item{
  padding-top: 24rpx;
  line-height: 1.2;
  font-size: 28rpx;
}
.order_rebate_left{
  width: 160rpx;
}
.order_rebate_rig{
  color: #333;
}
.th_order{
  text-align: right;
  padding-right: 24rpx;
}
.screen_wrap{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
}
.screen_mask{
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0,0.2);
  position: absolute;
  left: 0;
  top: 0;
}
.screen_box{
  min-height: 550rpx;
  background: #fff;
  width: 100%;
  position: fixed;
  bottom: 0;
  left: 0;
}
.screen_title{
  height: 74rpx;
  line-height: 74rpx;
  border-bottom: 1rpx solid #F8F8F8;
  padding: 0 24rpx;
}
.screen_title view{
  font-size: 30rpx;
}
.clear_btn{
  text-align: right;
}
.screen_item{
  padding: 40rpx 0 4rpx 24rpx;
}
.withdrawalState{
  padding-top: 4rpx;
}
.screen_left{
  width: 144rpx;
  font-size: 28rpx;
}
.screen_rig{
  font-size: 28rpx;
}
.screen_btn{
  display: inline-block;
  padding: 6rpx 24rpx;
  border: 1rpx solid #333;
  border-radius: 28rpx;
  margin-left: 8rpx;
  margin-right: 8rpx;
  margin-bottom: 16rpx;
  color: #333;
}
.screen_btn_a{
  border: 1rpx solid #FF3030;
  color: #FF3030;
}
.query_btn{
  width: 180rpx;
  height: 52rpx;
  line-height: 52rpx;
  background: #FF3030;
  border-radius: 26rpx;
  font-size: 30rpx;
  color: #FFFFFF;
  text-align: center;
  margin: 40rpx auto 0;
}
.picker_btn{
  display: inline-block;
  width: 200rpx;
  height: 44rpx;
  line-height: 44rpx;
  color: #777;
  border: 1rpx solid #333333;
  border-radius: 6rpx;
  text-align: center;
}