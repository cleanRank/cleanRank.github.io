.myFans{
  background:#fff;
  box-shadow:0 0 12rpx 0 rgba(0,0,0,0.08);
  border-radius:12rpx;
  margin-left: 24rpx;
  margin-right: 24rpx;
  margin-top: 30rpx;
  padding: 20rpx;
  margin-bottom: 40rpx;
  overflow: hidden;
  position: relative;
}
.myFansSum{
  text-align: center;
}
.hintImg{
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  right: 24rpx;
  top: 24rpx;
}
.myFansSum text{
  display: block;
}
.myFansSum .myFansNum {
  font-size: 80rpx;
  line-height: 112rpx;
  font-weight: 500;
}
.myFansTxt{
  font-size: 28rpx;
  line-height: 40rpx;
}
.fansOrder{
  justify-content: space-between;
  padding-top: 16rpx;
}
.fansOrderItem{
  text-align: center;
  width: 276rpx;
  padding-bottom: 4rpx;
}
.fansOrderItem text{
  display: block;
}
.fansOrderItem .fansOrderItemNum{
  font-size: 34rpx;
  line-height: 48rpx;
  font-weight: 500;
}
.fansOrderItem .fansOrderItemTxt{
  font-size: 24rpx;
  line-height: 33rpx;
}
/* fans list start */
.fansListWrap{
  padding: 0 24rpx;
  background-color: #fff;
  overflow: hidden;
}
.fansListScroll{
  background-color: #fff;
  overflow: hidden;
  min-height: 700rpx;
  padding-top: 30rpx;
}
.fansNav{
  margin-bottom: 30rpx;
  width: 100%;
  background-color: #fff;
}
.fixed{
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9;
}
.fansNavItem{
  color: #777;
  font-size: 28rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
}
.fansNavItem.active{
  color: #333;
  font-weight: 500;
  position: relative;
}
.fansNavItem.active:after{
  content: '';
  width: 40rpx;
  height: 3rpx;
  background-color: #FF3030;
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  bottom: 0;
}

.fansItem{
  align-items: center;
  margin-bottom: 60rpx;
}

.fansPhoto{
  width: 132rpx;
  height: 132rpx;
  border: 4rpx solid #f8f8f8;
  display: block;
  border-radius: 50%;
  margin-right: 30rpx;
}
.fansNameWrap{
  height: 48rpx;
  align-items: center;
}
.fansNameWrap text{
  display: block;
}
.fansNameWrap .fansName {
  max-width: 375rpx;
  font-size: 34rpx;
  font-weight: 600;
  line-height: 48rpx; 
  /* display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1; */
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.fansNameWrap .fansStatus{
  font-size: 24rpx;
  margin-left: auto;
}
.fansStatus.fansStatus1{
  color: #FF6330;
}
.fansInvite{
  font-size: 24rpx;
  justify-content: flex-start;
  margin-bottom: 2rpx;
}
.fansInvite>text{
  display: block;
}
.fansInviteShortNum{
  padding: 0 5rpx;
  margin-left: 20rpx;
}
.weight600{
  font-weight: 600;
  padding: 0 5rpx;
}
.fansBindTime{
  font-size: 24rpx;
}
.ImEntry{
  position: absolute;
  bottom: 4rpx;
  right: 0;
}
.ImEntry .ImEntryCon{
  height: 25rpx;
  font-size: 18rpx;
  color: #FF5A60;
  line-height: 25rpx;
  text-align: center;
  border: 1px solid #FD455F;
  border-radius: 20rpx;
  padding: 7rpx 18rpx 6rpx;
}
.ImEntry .unread{
  font-size: 18rpx;
  line-height: 25rpx;
  text-align: center;
  border-radius: 20rpx;
  color: #FFFFFF;
  background: #FD455F;
  box-sizing: border-box;
  padding: 7rpx 18rpx 6rpx;
}
/* fans list end */
.mask{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  justify-content: center;
  align-items: center;
  background: rgba(0,0,0,0.7)
}
.popup{
  width: 590rpx;
  height: 390rpx;
  background-color: #fff;
  border-radius: 12rpx;
  padding: 40rpx;
  flex-direction: column;
}
.mb-32{
  margin-bottom: 32rpx;
}
.note{
  line-height: 40rpx;
  font-size: 28rpx;
}
.weight500{
  font-weight: 500;
}
.popupClose{
  width: 180rpx;
  height: 52rpx;
  line-height: 52rpx;
  text-align: center;
  border-radius: 26rpx;
  background-color: #FF3030;
  color: #fff;
  font-size: 30rpx;
  margin-top: auto;
  margin-left: auto;
  margin-right: auto;
}
.browseUser{
  font-size: 24rpx;
  line-height: 33rpx;
  text-align: center;
  padding-top: 30rpx;
  padding-bottom: 40rpx;
}
.isMember{
  color: #DA9F4A;
  font-size: 18rpx;
  margin-left: 10rpx;
}