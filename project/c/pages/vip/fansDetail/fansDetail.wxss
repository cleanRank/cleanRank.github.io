.fansInfoWrap{
  padding: 24rpx 24rpx 30rpx 24rpx;
  align-items: center;
}
.fansInfoWrap .fansInfoImg{
  width: 134rpx;
  height: 134rpx;
  border-radius: 50%;
  border: 3rpx solid #F8F8F8;
  margin-right: 30rpx;
}
.fansNameWrap{
  height: 42rpx;
  margin-bottom: 4rpx;
  justify-content: space-between;
}
.fansNameWrap>text{
  display: block;
}
.fansName{
  overflow: hidden;
  font-size: 0;
  line-height: 42rpx; 
  flex: 1;
  display: flex;
}
.fansNameTxt{
  font-size: 30rpx;
  font-weight: 600;
  line-height: 42rpx; 
  display: block;
  max-width: 200rpx;
}
.fansLevel{
  color: #FF6330;
  font-size: 24rpx;
  margin-left: 30rpx;
  display: block;
}
.fansBuyNum{
  font-size: 24rpx;
  height: 33rpx;
  line-height: 33rpx;
  align-self: center;
  width: 150rpx;
  text-align: right;
}
.fansRebate{
  font-size: 24rpx;
  margin-bottom: 4rpx;
}
.fansRebateColor{
  color: #FF3030;
}
.fansBindTime{
  font-size: 24rpx;
}
.line{
  height: 10rpx;
  background: #F5F5F5;
}
/* order rebase start */
.orderRebate{
  padding: 40rpx 24rpx 24rpx 24rpx;
}
.orderRebateTit{
  height: 42rpx;
  line-height: 42rpx;
  font-size: 30rpx;
  margin-bottom: 24rpx;
}
.orderRebateTit>text{
  font-size: 24rpx;
}
.orderItem{
  margin-bottom: 40rpx;
  box-shadow:0px 8rpx 8rpx 0 rgba(0,0,0,0.04),0px 0 12rpx 0 rgba(0,0,0,0.08);
  border-radius:12rpx;
  background-color: #fff;
  padding-left: 24rpx;
  padding-right: 24rpx;
  padding-bottom: 20rpx;
}
.orderNoWrap{
  height: 76rpx;
  line-height: 76rpx;
  justify-content: space-between;
  align-items: center;
  border-bottom: 2rpx solid #F8F8F8;
  margin-bottom: 20rpx;

}
.orderNoWrap>text{
  display: block;
}
.orderNo{
  font-size: 24rpx;
}
.copy_btn{
  padding: 0 16rpx;
  line-height: 34rpx;
  height: 34rpx;
  font-size: 20rpx;
  border: 1px solid #333;
  border-radius: 18rpx;
}
.orderGood{

}
.orderGoodImg {
  width: 158rpx;
  height: 158rpx;
  border: 1px solid #E2E2E2;
}
.orderGoodInfo{
  margin-left: 20rpx;
  margin-bottom: 20rpx;
}
.orderGoodInfo .orderGoodName{
  height: 80rpx;
  line-height: 40rpx;
  font-size: 30rpx;
  display:-webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;
  overflow:hidden;
  text-overflow:ellipsis;
  margin-bottom: 8rpx;
}
.orderGoodEarning{
  color: #FF9E15;
  font-size: 24rpx;
  margin-bottom: 4rpx;
}
.orderAmount{
  justify-content: space-between;
  line-height: 33rpx;
  font-size: 24rpx;
}
.orderAmountPrice{
  
}
.totalRebate,.totalPriceWrap{
  justify-content: space-between;
}
.totalPriceWrap{
  margin-bottom: 16rpx;
}
.totalRebate{
  line-height: 33rpx;
}
.totalRebate .totalRebateTxt{
  font-size: 24rpx;
}
.totalRebateTxt .blod{
  font-weight: 500;
}
.totalRebate .totalRebateStatus{
  font-size: 24rpx;
}
.totalPriceWrap{
  line-height: 33rpx;
}
.totalPriceWrap .totalPrice{
  font-size: 36rpx;
  flex-flow: 600;
}
.totalPriceWrap .totalNum{
  font-size: 24rpx;
}
.orderTime{
  font-size: 24rpx;
}
/* order resbase end */