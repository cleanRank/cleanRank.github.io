  /* 无数据缺省 */
.noData{
    display: flex;
    justify-content: flex-start;
    flex-direction: column;
    align-items: center;
}
.noData image{
    width: 320rpx;
    height: 215rpx;
    margin:300rpx 0 96rpx;
}
.noData .noDataInfo{
    font-size: 28rpx;
    height: 40rpx;
    color: #999;
}
