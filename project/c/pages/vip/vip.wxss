
.vip_wrap_page{
  padding-bottom: 30rpx;
}
page{
  display:flex;
  flex-direction:column;
  height:calc(100vh);
}
.scrollCla{
  height: 100%;
  overflow: initial;
}
.vip_message_wrap{
  padding: 45rpx 24rpx 40rpx 24rpx;
  position: relative;
}
.vip_message_shareStore{
  position: absolute;
  right: 0;
  bottom: 41rpx;
  width: 126rpx;
  height: 40rpx;
  background: linear-gradient(135deg,rgba(252,109,109,1) 0%,rgba(245,70,70,1) 100%);
  font-size: 24rpx;
  color: #FFFFFF;
  text-align: center;
  line-height: 40rpx;
  border-radius: 40rpx 0 0 40rpx;
  z-index: 10;
}
.find_people{
  bottom: 90rpx;
  background: #DA9F4A;
  color: #fff;
}
.vip_header_box{
  width: 132rpx;
  height: 132rpx;
  top: 50%;
  margin-top: -66rpx;
  position: absolute !important;
}
.vip_header_img{
  width: 132rpx;
  height: 132rpx;
  border-radius: 50%;
}
.vip_rank{
  position: absolute;
  width: 30rpx;
  height: 30rpx;
  bottom: 5rpx;
  right: 5rpx;
}
.message_box{
  /* margin-left: 30rpx; */
  padding-left: 162rpx;
}
.nickName_box{
  height: 59rpx;
  font-size: 0;
  margin-bottom: 4rpx;
  align-items: center;
  /* justify-content: space-between; */
}
.nickName_box .nickName{
  margin-right: 24rpx;
  max-width: 240rpx;
}
.nickName_box text{
  font-size: 42rpx;
  font-weight: 600;
  display: block;
}
/* im入口 start*/
/* .nickNameWrap{
  display:flex;
} */
.imEntry{
  width: 110rpx;
  height: 56rpx;
  position: absolute;
  border-radius: 47rpx;
  box-shadow: 0 0 12rpx #FFD9DA;
  top: 45rpx;
  right: 20rpx; 
  z-index: 99;
  display:flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
}
.imEntry image{
  width: 42rpx;
  height: 41rpx;
}
.imEntry .redDot{
  position: absolute;
  top: 0;
  right: 32rpx;
  z-index: 20;
  width: 14rpx;
  height: 14rpx;
  border-radius: 7rpx;
  background: #FD455F;
}
/* im入口 end */
.user_grade{
  color: #fff;
  font-size: 22rpx;
  overflow: hidden;
  position: relative;
  background: linear-gradient(#575B64,#333333);
  padding: 0 15rpx;
  height: 40rpx;
  line-height: 40rpx;
  border-radius: 20rpx;
  text-align: center;
}
.user_grade .picker{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.Invitecode_boc{
  font-size: 24rpx;
  line-height:38rpx;
}
.Invitecode_box{
  border: 1rpx solid #333;
  /* width: auto; */
  height: 38rpx;
  line-height: 38rpx;
  border-radius: 8rpx;
  display: inline-flex;
  overflow: hidden;
  box-sizing: border-box;
}
.Invitecode_txt{
  font-size: 22rpx;
  color: #333;
  width: 100%;
  margin-left: 16rpx;
}
.Invitecode_copyBtn{
  width: 106rpx;
  text-align: center;
  background-color: #333;
  font-size: 22rpx;
  margin-left: -6rpx;
  color: #fff;
}
.pName_box{
  line-height: 40rpx;
  margin-bottom: 12rpx;
  display: flex;
  justify-content: space-between;
}
.pName_inviter {
  font-size: 24rpx;
  margin-right: 40rpx;
  width: 420rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow:ellipsis;
}
