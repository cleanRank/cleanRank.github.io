.sharing_shop_btn{
  color: #FF3030;
}
.fans_info_wrap, .expect_revenue_wrap{
  width: 702rpx;
  background: #FFFFFF;
  box-shadow: 0 0 12rpx 0 rgba(0,0,0,0.08);
  border-radius: 12rpx;
  margin: 47rpx auto 30rpx;
  text-align: center;
  padding: 20rpx 0 30rpx;
}
.account_balance_wrap{
  margin-bottom: 0;
}
.fans_info_wrap view{
  line-height: 1.6;
}
.fans_num_box{
  border-right: 1rpx solid #D8D8D8;
}
.num_fans{
  font-size: 34rpx;
}
.fans_txt{
  font-size: 28rpx;
}
.expect_revenue_wrap{
  margin: 30rpx auto;
  padding: 0;
}
.expect_revenue_title{
  text-align: left;
  border-bottom: 2rpx solid #f8f8f8;
  font-size: 30rpx;
  height: 72rpx;
  line-height: 72rpx;
  margin-left: 24rpx;
  position: relative;
}
.expect_revenue_title::after{
  content: ">";
  position: absolute;
  right: 24rpx;
  color: #777;
  font-size: 30rpx;
}
.expect_revenue_list{
  padding: 24rpx 0;
  border-bottom: 2rpx solid #f8f8f8;
}
.expect_revenue_list:last-child{
  border: none;
}
.expect_revenue_list view{
  line-height: 1.8;
}
.expect_revenue_item{
  position: relative;
}
.expect_revenue_item::after{
  content: '';
  width: 2rpx;
  height: 150rpx;
  background: #f8f8f8;
  position: absolute;
  top: -23rpx;
  right: 0;
}
.account_balance_wrap{
  text-align: left;
  padding: 16rpx 0 20rpx 24rpx;
}
.account_balance_title{
  font-size: 30rpx;
  margin-bottom: 14rpx;
}
.account_balance_num{
  font-size: 34rpx;
}
.go_cashingMoney{
  padding-right: 24rpx;
}
.go_cashingMoney_btn{
  margin-top: 36rpx;
}
/* vip seo start */
.share_box{
  height: 280rpx;
  font-size: 0;
  margin-left: 24rpx;
  margin-right: 24rpx;
  margin-bottom: 40rpx;
  overflow: hidden;
  border-radius: 12rpx;
}
.share_box image{
  width: 100%;
  height: 280rpx;
  margin: 0 auto;
  display: block;
}
.myVipWrap{
  margin-left: 24rpx;
  margin-right: 24rpx;
  box-shadow:0px 8rpx 8rpx 0px rgba(0,0,0,0.04),0px 0px 12rpx 0px rgba(0,0,0,0.08);
  border-radius:12rpx;
  background-color: #fff;
  margin-bottom: 40rpx;
  padding-bottom: 20rpx;
}
.myVipTit{
  height: 72rpx;
  line-height: 72rpx;
  border-bottom: 2rpx solid #F8F8F8;
  font-size: 30rpx;
  margin-left: 24rpx;
  align-items: center;
  justify-content: space-between;
}
.rightArrow{
  width: 64rpx;
  height: 64rpx;
  display: block;
}
.myFansClassify{
  overflow: hidden;
}
.myFansClassifyItem{
  flex: 1;
  text-align: center;
  margin-top: 20rpx;
  position: relative;
}
.borderRight:after{
  position: absolute;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  content: '';
  width: 2rpx;
  height: 76rpx;
  background-color: #F8F8F8;
}
.myFansClassifyItemNum,.myFansClassifyItemTxt{
  display: block;
  margin-bottom: 4rpx;
}
.myFansClassifyItemNum{
  font-size: 34rpx;
  font-weight: 600;
  line-height: 48rpx;
}
.myFansClassifyItemTxt{
  font-size: 28rpx;
  line-height: 40rpx;
}
/* earning start */
.myEarningTime{
  width: 436rpx;
  height: 244rpx;
  border-right: 2rpx solid #F8F8F8;
  margin-left: 24rpx;
}
.myEarningTimeItem{
  text-align: center;
}
.myEarningTimeItem .myEarningPrice{
  font-size: 30rpx;
  font-weight: 600;
  line-height: 42rpx;
  display: block;
  margin-bottom: 4rpx;
}
.myEarningTimeDay .myEarningPrice{
  font-size: 46rpx;
  line-height: 65rpx;
  margin-bottom: 2rpx;
}
.myEarningTimeItem .myEarningTxt{
  font-size: 24rpx;
  line-height: 33rpx;
  display: block;
}
.myEarningTimeDay{
  width: 100%;
  border-bottom: 2rpx solid #F8F8F8;
  padding: 20rpx 0;
}
.myEarningTimeMonth,.myEarningTotal{
  width: 50%;
  padding-top: 20rpx;
}
.earningWithdrawal{
  width: 240rpx;
  text-align: center;
  overflow: hidden;
}
.earningWithdrawal text{
  display: block;
}
.earningWithdrawalPrice{
  font-size: 34rpx;
  line-height: 48rpx;
  font-weight: 600;
  margin-top: 51rpx;
  margin-bottom: 10rpx;
  padding: 0 10rpx;
}
.earningWithdrawalTxt{
  font-size: 24rpx;
  line-height: 33rpx;
  margin-bottom: 40rpx;
}
.earningWithdrawalBtn{
  width: 100rpx;
  height: 46rpx;
  line-height: 46rpx;
  text-align: center;
  font-size: 28rpx;
  border:1px solid #FF3030;
  border-radius: 23rpx;
  margin: 0 auto;
  box-sizing: border-box;
}
.noticeWrap{
  height: 90rpx;
  padding-bottom: 0;
  overflow: hidden;
  line-height: 90rpx;
}
.noticeText{
  font-size: 26rpx;
  border-bottom:none;
  height: 100%;
  line-height: 90rpx;
  margin-left:0;
  overflow: hidden;
}
.noticeText image,.noticeText text{
  float:left;
}
.noticeText .rightArrow{
  float: right;
  margin-top: 13rpx;
  border:none;
}
.noticeText text{
  width:517rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.sipBox{
  height: 90rpx;
  overflow: hidden;
  margin-bottom: 40rpx;
  box-shadow:0px 8rpx 8rpx 0px rgba(0,0,0,0.04),0px 0px 12rpx 0px rgba(0,0,0,0.08);
}
.noticeIcon{
  width: 59rpx;
  height: 51rpx;
  margin-top: 19.5rpx;
  float: left;
  margin-left: 21rpx
}
.announceSwiper{
  float: right;
  width:604rpx;
  height: 100%;
}
/* earning end */
/* vip seo end */