page, .page {
  background-color: #f5f5f5;
}

.address {
  margin: 10rpx 0;
  background-color: #fff;
}

.addressContent {
  display: flex;
  flex-wrap: wrap;
  font-size: 30rpx;
  flex-direction: column;
  padding-top: 30rpx;
  padding-bottom: 60rpx;
}

.addressNaT {
  margin-left: 45rpx;
  display: flex;
  flex-direction: row;
}

.addressDetail {
  font-size: 26rpx;
  color: #777;
  margin-left: 45rpx;
  margin-right: 30rpx;
  padding-top: 18rpx;
}

.consigneeName, .consigneeTel {
  margin-right: 30rpx;
  line-height: 45rpx;
  font-size: 30rpx;
  color: #222;
}

.addressEdit {
  height: 70rpx;
  display: flex;
  border-top: 1px solid #e2e2e2;
  border-bottom: 1px solid #e2e2e2;
  justify-content: space-between;
  align-items: center;
  flex-direction: row;
  font-size: 26rpx;
}

.selectDefaultAddress {
  display: flex;
  align-items: center;
  flex-direction: row;
  height: 100%;
  margin-left: 30rpx;
}

