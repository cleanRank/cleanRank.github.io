.addressEditBtns {
  display: flex;
  color: #333;
  font-size: 26rpx;
  align-items: center;
  flex-direction: row;
}

.addressEditBtns .btn {
  margin-left: 10rpx;
  margin-right: 30rpx;
  color: #999;
  font-size: 24rpx;
}

.addressEditBtns .addressEditBtn-img {
  width: 40rpx;
  height: 40rpx;
}

.poition_btn{
  position: absolute;
  bottom: 70rpx;
}
.btns {
  margin: 55rpx 55rpx 55rpx 55rpx;
}

.save {
  background: #FD455F;
  width: 640rpx;
  height: 78rpx;
  text-align: center;
  line-height: 78rpx;
  font-size: 28rpx;
  color: #fff;
  border: none;
  border-radius: 39rpx;
}
.nodata_img{
  width:320rpx;
  height: 194rpx;
}
.nodata_txt{
  color: #999;
  margin-top: 82rpx;
  font-size: 28rpx;
}
