
/* navigation */
.navigationView{
  width: 100%;
  position: relative;
  /* margin-bottom: 16rpx!important; */
}
.navigation{
  display: flex;
  flex-wrap: wrap;
  margin-left: 20rpx;
  margin-right: 20rpx;
    justify-content: space-around;
}

.navigationItem{
  width: 96rpx;
  text-align:center;
  margin-bottom: 20rpx; 
}
.navigationItem image{
  /* margin: 0; */
  width: 90rpx;
  height: 90rpx;
  margin-bottom: 12rpx;
}

.navigationItem text{
  font-size: 24rpx;
  width: 100%;
  display: block;
  height: 33rpx;
  line-height: 33rpx;
  color: #444;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}