/* 超出配送范围 start */
.beyondScoped{
  position: fixed;
  top: 0;
  left:0;
  right: 0;
  bottom: 0;
  width: 100%;
  height: 100%;
  z-index: 9999;
  display:flex;
  justify-content: center;
  align-items: center;
}
.maskBg{
  position: absolute;
  top: 0;
  left:0;
  right:0;
  bottom: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.7);
  z-index: -1;
}
.beyongCon{
  width: 500rpx;
  background: #fff;
  border-radius: 12rpx;
  overflow: hidden;
  padding: 0 40rpx;
}
.beyongTitle{
  font-size: 30rpx;
  font-weight:600;
  line-height: 42rpx;
  padding: 32rpx  0;
  text-align:center;
}
.beyongInfo{
  font-size: 26rpx;
  color: #999;
  margin-bottom: 32rpx;
}
.beyondScopedList{
  width: 100%;
  min-height: 120rpx;
  max-height: 400rpx;
}
.beyondScopedList .scopedItem{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.beyondScopedList .scopedItem:not(:last-child){
  margin-bottom: 20rpx;
}
.scopedItem .scopedImg{
  width: 120rpx;
  height: 120rpx;
  overflow: hidden;
}
.scopedItem .scopedImg image{
  width: 100%;
  height: 100%;
}
.scopedItem .scopedInfo{
  width: 360rpx;
  height: 120rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.scopedName{
  font-size: 24rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.scopedPrice{
  font-size: 24rpx;
  color: #FF5A60;
}
.beyondBtn{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 40rpx 0;
}
.beyondBtn .btn{
  width: 240rpx;
  height: 78rpx;
  font-size: 30rpx;
  line-height: 78rpx;
  text-align: center;
  border-radius: 40rpx;
}
.beyondBtn .cancel{
  border: 1px solid #DBDBDB;
  background:#fff;
  color: #999;
}
.beyondBtn .sure{
  background: #FD455F;
  color: #fff;
}
.out_wrap{
  border-radius: 8rpx;
  margin: 20rpx 24rpx;
}
/* 超出配送范围 end */