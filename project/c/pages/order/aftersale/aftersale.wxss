.page .content1-item-list-btns view {
  width: 180rpx;
  height: 50rpx;
  line-height: 50rpx;
  border: 1px solid #FF3030;
  color: #FF3030;
  margin-top: 25rpx;
  font-size: 28rpx;
  text-align: center;
  border-radius: 27rpx;
  margin-right: 25rpx;
}
.page .content1-item-list-btns .cancel {
  background: #FF3030;
  color: #fff;
}
