page {
  background: #F4F4F4;
}
.page {
  /* 搜索按钮 */
}
.page .head {
  display: flex;
  height: 72rpx;
  line-height: 72rpx;
  background: #fff;
}
.page .head view {
  flex: 1;
  font-size: 30rpx;
  color: #9F9F9F;
  text-align: center;
}
.page .head .active {
  color: #FF3030;
  position: relative;
}
.page .head .active::after {
  content: '';
  background: #FF3030;
  position: absolute;
  width: 40%;
  height: 4rpx;
  bottom: 0;
  left: 30%;
}
.page .searchHeader {
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #fff;
  padding: 16rpx 24rpx;
}
.page .searchArea {
  width: 100%;
  max-width: 616rpx;
}
.page .searchBtn {
  color: #777777;
  font-size: 30rpx;
}
.page scroll-view {
  position: absolute;
  width: 100%;
}
.page .content-item {
  margin-top: 16rpx;
  background: #fff;
}
.page .content-item-head {
  height: 90rpx;
  line-height: 90rpx;
  font-size: 28rpx;
  padding: 0 24rpx;
  display: flex;
  justify-content: space-between;
}
.page .content-item-head-left {
  color: #333;
  text-align: left;
}
.page .content-item-head-right {
  text-align: right;
}
.page .content-item-list-item {
  border-top: 1px solid #E2E2E2;
  padding: 24rpx;
  position: relative;
}
.page .content-item-list-item image {
  position: absolute;
  left: 24rpx;
  top: 50%;
  transform: translate(0, -50%);
  width: 130rpx;
  height: 130rpx;
}
.page .content-item-list-item-right {
  padding-left: 147rpx;
  width: 555rpx;
}
.page .content-item-list-item-right-title {
  font-size: 26rpx;
  color: #333;
  overflow: hidden;
  word-break: keep-all;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.page .content-item-list-item-right-num {
  font-size: 24rpx;
  color: #777;
  margin-top: 8rpx;
}
.page .content-item-list-item-right-bottom {
  margin-top: 18rpx;
  display: flex;
  justify-content: space-between;
}
.page .content-item-list-item-right-bottom-price {
  font-size: 30rpx;
  color: #333;
}
.page .content-item-list-item-right-bottom-btn {
  width: 140rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  border: 1px solid #ccc;
  border-radius: 22rpx;
  font-size: 24rpx;
  color: #ccc;
}
.page .content-item-list-item-right-bottom .red {
  color: #FF3030;
  border-color: #FF3030;
}
