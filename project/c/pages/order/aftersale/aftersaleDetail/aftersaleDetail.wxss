page {
  background: #F4F4F4;
}
.page {
  font-size: 26rpx;
  color: #333;
}
.page-type {
  position: relative;
  padding: 24rpx;
  margin-top: 20rpx;
  background: #fff;
}
.page-type-money text {
  color: #FF3030;
}
.page-type-msg {
  color: #FF3030;
  font-size: 28rpx;
  position: absolute;
  right: 24rpx;
  top: 50%;
  transform: translate(0, -50%);
}
.page-order {
  margin-top: 20rpx;
  background: #fff;
}
.page-order-top {
  padding: 24rpx;
}
.page-order-bottom {
  border-top: 1px solid #E2E2E2;
  padding: 24rpx;
  min-height: 130rpx;
  position: relative;
}
.page-order-bottom image {
  position: absolute;
  left: 24rpx;
  top: 50%;
  transform: translate(0, -50%);
  width: 130rpx;
  height: 130rpx;
}
.page-order-bottom-right {
  padding-left: 147rpx;
  width: 555rpx;
}
.page-order-bottom-right-title {
  font-size: 26rpx;
  color: #333;
  overflow: hidden;
  word-break: keep-all;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.page-order-bottom-right-num {
  margin-top: 8rpx;
}
.page-order-bottom-right-info {
  margin-top: 18rpx;
  display: flex;
  justify-content: space-between;
}
.page-order-bottom-right-info-price {
  font-size: 30rpx;
}
.page-order-bottom-right-info-num {
  color: #777777;
  font-size: 24rpx;
}
.page-logistics {
  background: #fff;
  margin-top: 20rpx;
}
.page-logistics-title {
  height: 95rpx;
  line-height: 95rpx;
  padding: 0 24rpx;
  font-size: 28rpx;
}
.page-logistics-msg {
  border-top: 1px solid #E2E2E2;
  padding: 24rpx;
}
.page-logistics-msg-info {
  display: flex;
  justify-content: space-between;
  margin-top: 10rpx;
}
.page-logistics-msg-info-btn {
  width: 140rpx;
  height: 40rpx;
  line-height: 40rpx;
  border: 1px solid #FF3030;
  color: #FF3030;
  font-size: 24rpx;
  text-align: center;
  border-radius: 22rpx;
}
.page-address {
  margin-top: 20rpx;
  background: #fff;
}
.page-address-title {
  padding: 0 24rpx;
  height: 95rpx;
  line-height: 95rpx;
  font-size: 28rpx;
}
.page-address-info {
  border-top: 1px solid #E2E2E2;
  padding: 24rpx;
}
.page-address-info-item {
  display: flex;
  margin-bottom: 10rpx;
}
.page-address-info-item-left {
  width: 158rpx;
}
.page-address-info-item-right {
  width: 540rpx;
  color: #777;
}
.page-description {
  margin-top: 20rpx;
  background: #fff;
}
.page-description-title {
  padding: 0 24rpx;
  height: 95rpx;
  line-height: 95rpx;
  font-size: 28rpx;
}
.page-description-info {
  border-top: 1px solid #E2E2E2;
  padding: 24rpx;
}
.page-description-info-content {
  font-size: 28rpx;
  color: #999;
}
.page-description-info-img {
  margin-top: 20rpx;
  display: flex;
  justify-content: flex-start;
}
.page-description-info-img image {
  width: 200rpx;
  height: 200rpx;
  margin-right: 50rpx;
}
.page-description-info-img image:last-child {
  margin-right: 0;
}
.page-btn {
  height: 100rpx;
  background: #fff;
  margin-top: 20rpx;
  padding-right: 24rpx;
  display: flex;
  justify-content: flex-end;
}
.page-btn view {
  width: 180rpx;
  height: 50rpx;
  line-height: 50rpx;
  color: #FF3030;
  border: 1px solid #FF3030;
  border-radius: 27rpx;
  text-align: center;
  margin-top: 25rpx;
  margin-left: 25rpx;
}
