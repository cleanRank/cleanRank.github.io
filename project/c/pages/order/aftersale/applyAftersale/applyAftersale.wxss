page {
  background: #F4F4F4;
}
.page-head {
  padding: 24rpx;
  margin-top: 20rpx;
  background: #fff;
  position: relative;
}
.page-head image {
  position: absolute;
  left: 24rpx;
  top: 50%;
  transform: translate(0, -50%);
  width: 130rpx;
  height: 130rpx;
}
.page-head-right {
  padding-left: 147rpx;
  width: 555rpx;
}
.page-head-right-title {
  font-size: 26rpx;
  color: #333;
  overflow: hidden;
  word-break: keep-all;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.page-head-right-num {
  font-size: 24rpx;
  color: #777;
  margin-top: 8rpx;
}
.page-head-right-bottom {
  margin-top: 18rpx;
  display: flex;
  justify-content: space-between;
}
.page-head-right-bottom-price {
  font-size: 30rpx;
  color: #333;
}
.page-num {
  margin-top: 20rpx;
  padding: 30rpx 24rpx;
  background: #fff;
}
.page-num-title {
  color: #333;
  font-size: 28rpx;
}
.page-num-btn {
  margin-top: 30rpx;
  display: flex;
}
.page-num-btn-left {
  height: 50rpx;
  line-height: 46rpx;
  text-align: center;
  border: 1px solid #ccc;
  border-radius: 27rpx;
  font-size: 36rpx;
  color: #ccc;
  display: flex;
}
.page-num-btn-left-subtract {
  width: 50rpx;
}
.page-num-btn-left-count {
  width: 60rpx;
  line-height: 50rpx;
  border-left: 1px solid #ccc;
  border-right: 1px solid #ccc;
  font-size: 28rpx;
  color: #333;
}
.page-num-btn-left-add {
  width: 50rpx;
}
.page-num-btn-left .red {
  color: #FF3030;
}
.page-num-btn-right {
  color: #999;
  line-height: 52rpx;
  font-size: 28rpx;
  margin-left: 20rpx;
}
.page-type {
  margin-top: 20rpx;
  padding: 30rpx 24rpx;
  background: #fff;
  font-size: 28rpx;
  color: #333;
}
.page-type-title text {
  color: #999;
}
.page-type-btn {
  margin-top: 30rpx;
  margin-bottom: 30rpx;
  display: flex;
}
.page-type-btn view {
  width: 160rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  border: 1px solid #ccc;
  border-radius: 27rpx;
  margin-right: 24rpx;
}
.page-type-btn .active {
  color: #FF3030;
  border-color: #FF3030;
}
.page-type-money {
  margin-top: 30rpx;
  margin-bottom: 30rpx;
  line-height: 50rpx;
  display: flex;
}
.page-type-money .text {
  color: #FF3030;
}
.page-type-money input {
  color: #FF3030;
}
.page-type-info {
  background: #f4f4f4;
  color: #999;
  margin-left: -24rpx;
  margin-right: -24rpx;
  padding-left: 24rpx;
  height: 78rpx;
  line-height: 78rpx;
}
.page-type-cause {
  margin-top: 20rpx;
}
.page-type-cause-info {
  font-size: 26rpx;
  color: #999;
  margin-top: 20rpx;
  position: relative;
}
.page-type-cause-info-img {
  position: absolute;
  right: 0;
  top: 50%;
  transform: translate(0, -50%);
  padding: 20rpx;
}
.page-type-cause-info-img image {
  width: 44rpx;
  height: 44rpx;
}
.page-type-cause-info-img .picker {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.page-cause {
  margin-top: 20rpx;
  padding: 30rpx 24rpx;
  background: #fff;
  font-size: 28rpx;
  color: #333;
}
.page-cause-text {
  margin-top: 30rpx;
  border: 1px solid #ccc;
  height: 368rpx;
  padding: 24rpx;
  border-radius: 10rpx;
}
.page-cause-text textarea {
  width: 100%;
}
.page-cause-text-info {
  text-align: right;
}
.page-uploadImg {
  margin-top: 20rpx;
  padding: 30rpx 0;
  background: #fff;
  font-size: 28rpx;
  color: #333;
}
.page-uploadImg-title {
  padding-left: 24rpx;
}
.page-uploadImg .ui_uploader_cell {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
}
.page-uploadImg .ui_uploader_item {
  width: 200rpx;
  height: 200rpx;
  padding: 25rpx;
  position: relative;
}
.page-uploadImg .ui_uploader_item_icon {
  position: absolute;
  right: 6rpx;
  top: 6rpx;
  width: 32rpx;
  height: 32rpx;
}
.page-uploadImg .ui_uploader_item .showImg {
  width: 100%;
  height: 100%;
}
.page-uploadImg .ui_uploader {
  width: 200rpx;
  padding: 25rpx;
  height: 200rpx;
}
.page-uploadImg .ui_uploader image {
  width: 100%;
  height: 100%;
}
.page-btn {
  margin-top: 20rpx;
  height: 100rpx;
  line-height: 100rpx;
  background: #FF3030;
  color: #fff;
  text-align: center;
  font-size: 34rpx;
}
.page-model {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 99;
  background: rgba(0, 0, 0, 0.5);
}
.page-model-main {
  position: absolute;
  bottom: 0;
  width: 100%;
  background: #fff;
}
.page-model-main-head {
  font-size: 34rpx;
  color: #333;
  margin: 30rpx 0;
  text-align: center;
}
.page-model-main-content {
  margin-top: 30rpx;
}
.page-model-main-content-item {
  display: flex;
  justify-content: space-between;
  border-bottom: 1px solid #E2E2E2;
  padding: 0 24rpx;
  font-size: 30rpx;
  line-height: 90rpx;
}
.page-model-main-content-item:last-child {
  border: none;
}
.page-model-main-close {
  margin-top: 20rpx;
  height: 100rpx;
  line-height: 100rpx;
  background: #FF3030;
  color: #fff;
  text-align: center;
  font-size: 34rpx;
}
.page-model .checkbox-out {
  width: 44rpx;
  height: 44rpx;
  margin-top: 21rpx;
  margin-right: 20rpx;
  border: 1px solid #ccc;
  border-radius: 50%;
  position: relative;
}
.page-model .active {
  border-color: #FF3030;
}
.page-model .active::after {
  content: "";
  position: absolute;
  top: 50%;
  left: 50%;
  width: 65%;
  height: 65%;
  transform: translate(-50%, -50%);
  background: #FF3030;
  border-radius: 50%;
}
