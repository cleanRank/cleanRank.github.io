.floorPage{
  padding: 0rpx 24rpx;
}
.twoImg,.threeImg,.fourImg{
  display: flex;
  margin-bottom: 20rpx;
}
.twoImg view{
  width: 344rpx;
  height:258rpx;
  overflow: hidden;
  border-radius:8rpx;
  margin-right: 20rpx;
}
.twoImg image,.threeImg image,.fourImg image{
  display: block;
  width: 100%;
  height: 100%;
}
.threeImg view{
  width: 226rpx;
  height: 170rpx;
  border-radius: 5rpx;
  margin-right: 12rpx;
  overflow: hidden;
}
.fourImg view{
  width: 168rpx;
  height: 126rpx;
  margin-right: 10rpx;
  border-radius: 4rpx;
  overflow: hidden;
}
.twoImg view:last-child,.threeImg view:last-child,.fourImg view:last-child{
  margin-right:0
}