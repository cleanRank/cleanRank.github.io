@import "../../../index/wxss/navigation.wxss";
@import "../../../index/wxss/seckill.wxss"; 
@import "../../../index/wxss/Firstbanner.wxss";
@import "../../../../app.wxss";
.floor-swiper{
  width: auto;
  height: 311rpx;
  overflow-x: scroll;
  margin-bottom: 20rpx;
}
.nav-1-left {
  white-space: nowrap;
  overflow: hidden;
  flex: 0 0 100%;
  border: none;
  margin-left: 24rpx;
}
.floor-scroll-item{
  display: inline-block;
  width: 200rpx;
  height: 311rpx;
  background: #fff;
  border-radius: 8rpx;
  margin-right: 12rpx;
  padding-top: 12rpx;
  padding-bottom: 12rpx;
  box-sizing: border-box;
  overflow: hidden;
}
.floor-scroll-item image{
  display: block;
  width: 200rpx;
  height: 200rpx;
  margin: auto;
  margin-bottom: 16rpx;
}
.swiper-tit,.swiper-subtit{
  font-size: 24rpx;
  color:#444;
  line-height: 33rpx;
  width: 100%;
  overflow: hidden;
  text-align: center;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.swiper-subtit{
  font-size: 28rpx;
  color: #FF5A60;
  line-height: 34rpx;
  margin-top: 4rpx;
}
.floorFourBan{
  width: 702rpx;
  height: 320rpx;
  margin: auto;
  margin-bottom: 12rpx;
}
.floorFourBan image{
  display: block;
  width: 100%;
  height: 100%;
}