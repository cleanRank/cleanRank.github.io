/* 搜索商品 */
.totalBg {
  background: #F4F4F4;
  /* height: 1218rpx; */
  /* height: 100%; */
}

page {
  /* height: 100%; */
}

.totalBg .mod {
  margin-bottom: 20rpx;
}

.pages {
  width: 640rpx;
  height: 60rpx;
  background: rgba(245, 245, 245, 1);
  border-radius: 31rpx;
  font-size: 28rpx;
  margin: 10rpx 0rpx 35rpx 0;
  padding: 0 30rpx;
}

.nopad {
  padding-bottom: 0 !important;
}

.page {
  display: flex;
  justify-items: center;
  align-items: center;
}

.title {
  /* line-height:60rpx; */
  font-size: 28rpx;
  width: 92%;
}

.text {
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.fixed-width-height {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}

.inner {
  position: fixed;
  bottom: 100rpx;
  width: 100%;
  max-height: 600rpx;
  z-index: 1;
  /* overflow-y:scroll; */
}

.noData {
  display: flex;
  align-items: center;
  flex-direction: column;
  margin-top: 100rpx;
}

.nodata {
  font-size: 36rpx;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  color: rgba(102, 102, 102, 1);
  line-height: 50rpx;
}

.noData .img {
  width: 300rpx;
  height: 280rpx;
}

.commonSearch {
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
}

.localText {
  font-size: 30rpx;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  color: rgba(51, 51, 51, 1);
  margin-top: 25rpx;
}

.mg-l,
.mg-r {
  margin-left: 10rpx;
}

.searchGoods {
  padding: 0 30rpx;
  background: #fff;
  position: relative;
  /* margin-bottom:98rpx; */
}

.history {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.history1 {
  display: flex;
  align-items: center;
}

.shopHistory {
  line-height: 50rpx;
  background: rgba(242, 242, 242, 1);
  border-radius: 25rpx;
  text-align: center;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  display: inline-block;
  margin: 16rpx 20rpx 0 10rpx;
  min-width: 118rpx;
}

.scroll-height1 {
  padding-bottom: 98rpx;
  background: #fff;
  min-height: 100%;
}

.shopPic {
  width: 140rpx;
  height: 140rpx;
  margin-right: 20rpx;
}

.showShopList {
  display: flex;
  width: 100%;
  align-items: center;
  padding: 42rpx 0 22rpx;
}

.shopDescription {
  display: flex;
  width: 80%;
  flex-direction: column;
  min-height: 140rpx;
  justify-content: space-between;
}
.shopDescription .disFlex {
  display: flex;
  align-items: center;
  margin-top: 10rpx;
}
.shopDescription .discount {
  font-size: 20rpx;
  line-height: 30rpx;
  width: 80rpx;
  background: #F5A623;
  color: #fff;
  text-align: center;
  margin-right: 10rpx;
  border-radius: 10rpx;
}
.disprice {
  margin-left: 20rpx;
  line-height: 40rpx;
  font-size: 24rpx;
  color: #666;
  text-decoration:line-through;
}

.shopName {
  font-size: 30rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.price {
  font-size: 28rpx;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  color: rgba(255, 88, 75, 1);
  line-height: 41rpx;
}

.num {
  font-size: 26rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 65rpx;
}

.discount {
  font-size: 24rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(245, 166, 35, 1);
  line-height: 43px;
}

.loading {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  background-color: rgba(0, 0, 0, 0.3);
}

.bottomShopCar {
  padding: 0 20rpx;
  display: flex;
  align-items: center;
  position: fixed;
  bottom: 0;
  height: 98rpx;
  width: 100%;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px -3rpx 12rpx 0px rgba(204, 204, 204, 0.5);
  z-index: 2;
}

.totalPrice {
  flex: 5;
  margin-left: 20rpx;
  font-size: 34rpx;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  color: rgba(255, 88, 75, 1);
  line-height: 34rpx;
}

.selectedBtn {
  flex: 3;
  width: 280rpx;
  font-size: 36rpx;
  line-height: 100rpx;
  background: rgba(255, 48, 48, 1);
  color: #fff;
  text-align: center;
}

.shopCarNum {
  /* display: flex;
  justify-content: center;
  align-items: center; */
  line-height: 30rpx;
  text-align: center;
  top: 14rpx;
  left: 45rpx;
  position: absolute;
  min-width: 30rpx;
  min-height: 30rpx;
  font-size: 20rpx;
  background: rgba(255, 88, 75, 1);
  border-radius: 50%;
  color: #fff;
}

.searchHistory {
  padding: 10rpx 0 10rpx 10rpx;
  font-size: 28rpx;
  border-bottom: 1rpx solid #fff;
}

.searchWords {
  position: absolute;
  top: 60rpx;
  left: 95rpx;
  width: 80%;
  min-height: 100rpx;
  background: rgba(245, 245, 245, 1);
  color: rgba(102, 102, 102, 1);

}

.outerLi .fixedLi {
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  left: 0;
  top: 0;
}

.outerLi {
  position: relative;
}



.flexible {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.total {
  font-size: 30rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  line-height: 42rpx;
  margin-left: 10rpx;
}

.pos {
  position: fixed;
  width: 100%;
  z-index: 888;
  height: 80rpx;
}

.select {
  width: 38rpx;
  height: 38rpx;
}

.pd_3 {
  padding: 0 10rpx;
}

.del-empty {
  padding-right: 36rpx;
}

.goodlist-wrap {
  overflow-y: scroll;
  max-height: 520rpx;
}