page {
  background-color: #F3F3F3;
  height: 100%;
  width: 100%;
  overflow: scroll;
}

.coupon-header {
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 95rpx 0 33rpx;
  background: #ffffff;
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  z-index: 10;

}

.coupon-header .label {
  font-size: 28rpx;
  font-weight: 600;
  color: rgba(51, 51, 51, 1);
}

.coupon-header .check {
  width: 40rpx;
  height: 40rpx;
}

.ticket-list .check {
  width: 40rpx;
  height: 40rpx;
  margin-right: 58rpx;
}

.confirm-wrap {
  position: fixed;
  width: 100%;
  height: 122rpx;
  background-color: #fff;
  left: 0;
  bottom: 0;
  z-index: 66;
  display: flex;
  justify-content: center;
  align-items: center;
  box-shadow: 0px -5px 10rpx 0px rgba(215, 215, 215, 0.2);
}

.confirm {
  width: 643rpx;
  height: 90rpx;
  background: rgba(255, 48, 48, 1);
  line-height: 90rpx;
  /* margin: 100rpx auto 0; */
  color: #ffffff;
  font-size: 32rpx;
  border-radius: 45rpx;
  text-align: center;
}

.discount-detail {
  width: 100%;
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #666;
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
}

.discount-detail .swiper-tab-item {
  margin: 0 34rpx;
  padding: 9px 0 15rpx 0;
}


.discount-detail .active {
  /* width:72px; */
  color: rgba(255, 48, 48, 1);
  border-bottom: 4rpx solid #FF3030;
}

.show {
  display: block;
}

.hidden {
  display: none;
}

.ticket-list {
  padding-bottom: 200rpx;
  margin-top: 100rpx;

}

/* pages/fpages/ticketCenter/ticketCenter.wxss */
.page-bgd {
  background-color: #F3F3F3;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
}

.ticket-wrap {}

.ticket-bg-wrap {
  position: relative;
  width: 78rpx;
  height: 232rpx;
}

.ticket-bg-type {
  position: absolute;
  left: 23rpx;
  top: 48.5rpx;
  width: 32rpx;
  line-height: 43rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
}

.ticket-bg {
  position: absolute;
  left: 0;
  top: 0;
  flex: 78rpx 0;
  width: 78rpx;
  height: 232rpx;
  display: block;
}

.ticket-t {
  position: relative;
  display: flex;
  justify-content: space-between;
  margin: 20rpx 24rpx 0 24rpx;
}

.ticket-content {
  position: relative;
}

.ticket-r {
  background-color: #fff;
  flex: 1;
}

.ticket-t-price {
  padding: 10rpx;
  align-items: center
}

.ticket-t-price,
.ticket-t-time {
  display: flex;
  justify-content: space-between;
}

.price {
  width: 450rpx;

}

.price-detail {
  font-size: 90rpx;
  font-family: Helvetica;
}

.price-discount {
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
}

.price-detail-gray,
.price-discount-gray {
  color: #999;
}

.price-detail-red,
.price-discount-red {
  color: #FF714A;
}

.price-write {
  font-size: 30rpx;
  color: #333;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  margin-bottom: 10rpx;
  /* margin-top: 5rpx; */
}

.price-btn {
  font-size: 30rpx;
  color: #fff;
  width: 173rpx;
  height: 56rpx;
  border-radius: 30rpx;
  text-align: center;
  line-height: 56rpx;
  margin-right: 21rpx;
  /* margin-top: 61rpx; */
  font-family: PingFangSC-Regular;
  font-weight: 400;
}

.price-btn-red {
  background: rgba(255, 48, 48, 1);
}

.price-btn-gray {
  background: #C5C5C5;
}

.ticket-t-time {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 24rpx;
  color: #999;
  width: 624rpx;
  height: 65rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  border-top: 1px dashed rgba(204, 204, 204, 1);
}

.time {
  margin-left: 18rpx;
}

.rules {
  margin-right: 38rpx;
  display: flex;
  align-items: center;

}

.rules-write {}

.rules-btn {
  width: 30rpx;
  height: 30rpx;
  margin-left: 10rpx;
}

.rules-model {
  position: relative;
  width: 583rpx;
  /* height: 100rpx; */
  background-color: #fff;
  font-size: 20rpx;
  color: #999;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  line-height: 30rpx;
  padding: 19rpx 19rpx 18rpx 22rpx;
}

.rules-model view {}

.rules-model_left_top,
.rules-model_right_top,
.rules-model_right_bottom,
.rules-model_left_bottom {
  position: absolute;
  width: 13rpx;
  height: 13rpx;
  z-index: 1;
  background: #F3F3F3;
}

.rules-model_left_top {
  top: -1rpx;
  left: -1rpx;
  border-radius: 0rpx 0rpx 13rpx 0rpx;
}

.rules-model_right_top {
  top: -1rpx;
  right: -1rpx;
  border-radius: 0rpx 0rpx 0rpx 13rpx;
}

.rules-model_right_bottom {

  bottom: -1rpx;
  right: -1rpx;
  border-radius: 13rpx 0rpx 0rpx 0rpx;
}

.rules-model_left_bottom {

  bottom: -1rpx;
  left: -1rpx;
  border-radius: 0rpx 13rpx 0rpx 0rpx;
}

.hide {
  display: none;
}

.show {
  display: block;
}






