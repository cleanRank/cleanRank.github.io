  text-align: center;
  margin-right: 10rpx;
  border-radius: 10rpx;
}
.disprice {
  line-height: 40rpx;
  font-size: 24rpx;
  color: #666;
  text-decoration:line-through;
}
.shopName {
  font-size: 26rpx;
  color: rgba(51, 51, 51, 1);

  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;

}

.price {
  font-size: 28rpx;
  font-weight: 600;
  color: rgba(255, 88, 75, 1);
}

.num {
  font-size: 26rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  margin: 0 10rpx;
  width: 30rpx;
  text-align: center;

}

.footer {
  position: fixed;
  height: 98rpx;
  z-index: 12;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px -3rpx 12rpx 0rpx rgba(204, 204, 204, 0.5);
  display: flex;
  align-items: center;
  bottom: 0;
  width: 100%;
}

.outerLi .fixedLi {
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  left: 0;
  top: 0;
}

.outerLi {
  position: relative;
}

.footer .cart {
  position: relative;
  width: 45rpx;
  height: 44rpx;
  margin-left: 26rpx;
}

.footer .cart .img {
  width: 44rpx;
  height: 40rpx;
}

.footer .cart .em {
  position: absolute;
  min-width: 32rpx;
  height: 32rpx;
  /* padding: 0 2rpx; */
  line-height: 32rpx;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
  display: inline-block;
  background: #FF584B;
  color: #ffffff;
  border-radius: 50%;
  font-size: 20rpx;
  right: -16rpx;
  top: -16rpx;
}

.footer .price {
  font-size: 34rpx;
  margin-left: 50rpx;
  font-weight: 600;
  color: rgba(255, 88, 75, 1);
}

.footer .confirm {
  flex: 1;
  text-align: right;
  height: 100%;
  display: flex;
  justify-content: flex-end;
}

.confirm text {
  width: 280rpx;
  height: 100%;
  background-color: #FF3030;
  color: #ffffff;
  font-size: 36rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.no-data {
  height: 100%;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.no-data .img {
  width: 318rpx;
  height: 246rpx;
  margin-top: 100rpx;
}

/* 隐藏滚动条 */
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.loading {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  background-color: rgba(0, 0, 0, 0.3);
  z-index: 2;

}

.inner {
  position: fixed;
  bottom: 100rpx;
  width: 100%;
  max-height: 600rpx;
  z-index: 11;
  /* overflow-y:scroll; */
}

.fixed-width-height {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}

.scal {
  color: red;
  animation: scal .5s normal;
}

@keyframes scal {
  0% {
    transform: scale(1);
  }

  50% {
    transform: scale(2);
  }

  100% {
    transform: scale(1);
  }
}