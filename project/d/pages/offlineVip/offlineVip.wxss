  letter-spacing: 2rpx;
  margin-right: 25rpx;

}

.headImg {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  margin-right: 15rpx;
  overflow: hidden;
}

.gradeCard {
  margin-top: 20rpx;
  margin-left: 5rpx;
  position: relative;
  color: #fff;
}

.card {
  width: 702rpx;
  height: 341rpx;
  margin: 0 auto;
}

.cardInfo {
  position: absolute;
  top: 0;
  left: 30rpx;

}

.info-wrap {
  display: flex;
  justify-content: flex-start;
  height: 90rpx;
  margin-left: -46rpx;
}

.info {
  display: flex;
  flex-direction: column;
  margin-bottom: -4rpx;

}

.vipStyle {
  font-size: 40rpx;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  line-height: 56rpx;
