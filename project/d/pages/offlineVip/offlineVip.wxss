}
.headInfo {
  display: flex;
  align-items: center;
  position: relative;
  z-index: 1;
  margin-top: 20rpx;
}

.bgf {
  background: #fff;
} 

.vipLogo {
  width: 151rpx;
  height: 44rpx;
}

.userName {
  font-size: 36rpx;
  font-family: PingFangSC-Semibold;
